---
import type { HTMLAttributes } from "astro/types";

const { props } = Astro;

type Link = HTMLAttributes<"link">;
type Meta = HTMLAttributes<"meta">;
type MetaProps = {
  content?: string | URL | undefined | null;
  httpEquiv?: string | undefined | null;
  name?: string | undefined | null;
  property?: string | undefined | null;
};
---

{props.extend.link?.map((attributes: Link) => <link {...attributes} />)}
{
  props.extend.meta?.map(
    ({ content, httpEquiv, name, property }: MetaProps): Meta => (
      <meta
        content={content}
        http-equiv={httpEquiv}
        name={name}
        property={property}
      />
    )
  )
}
